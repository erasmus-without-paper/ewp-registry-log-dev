    <host>
        <ewp:admin-email>hello@academica.ro</ewp:admin-email>
        <ewp:admin-provider>academica-digital</ewp:admin-provider>
        <ewp:admin-notes>academicadigital.eu</ewp:admin-notes>
        <r:apis-implemented>
            <discovery
                xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v6/manifest-entry.xsd"
                version="6.0.0"
            >
                <url>https://app-staging.studemy.com/ewp/api/usamvcluj/discovery</url>
            </discovery>
            <echo
                xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v2/manifest-entry.xsd"
                version="2.0.0"
            >
                <http-security>
                    <sec:client-auth-methods>
                        <httpsig/>
                    </sec:client-auth-methods>
                    <sec:server-auth-methods>
                        <httpsig/>
                    </sec:server-auth-methods>
                </http-security>
                <url>https://app-staging.studemy.com/ewp/api/usamvcluj/echo</url>
            </echo>
        </r:apis-implemented>
        <institutions-covered>
            <r:hei id="usamvcluj.ro">
                <r:other-id type="erasmus">RO CLUJNAP04</r:other-id>
                <r:other-id type="pic">997851514</r:other-id>
                <r:name>University of Agricultural Sciences and Veterinary Medicine of Cluj-Napoca</r:name>
            </r:hei>
        </institutions-covered>
        <client-credentials-in-use>
            <rsa-public-key></rsa-public-key>
        </client-credentials-in-use>
        <server-credentials-in-use>
            <rsa-public-key></rsa-public-key>
        </server-credentials-in-use>
    </host>
</manifest>
