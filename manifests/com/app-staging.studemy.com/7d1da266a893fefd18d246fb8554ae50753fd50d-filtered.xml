    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v6/manifest-entry.xsd
        https://raw.githubusercontent.com/erasmus-without-paper/ewp-specs-api-discovery/stable-v6/manifest-entry.xsd

        https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v2/manifest-entry.xsd
        https://raw.githubusercontent.com/erasmus-without-paper/ewp-specs-api-echo/stable-v2/manifest-entry.xsd

        https://github.com/erasmus-without-paper/ewp-specs-api-iias/tree/stable-v7
        https://raw.githubusercontent.com/erasmus-without-paper/ewp-specs-api-iias/stable-v7/manifest-entry.xsd

        https://github.com/erasmus-without-paper/ewp-specs-api-iia-cnr/tree/stable-v3
