<ns3:manifest
    xmlns:ns3="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/tree/stable-v6"
    xmlns="https://github.com/erasmus-without-paper/ewp-specs-architecture/blob/stable-v1/common-types.xsd"
    xmlns:ns10="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v6/manifest-entry.xsd"
    xmlns:ns11="https://github.com/erasmus-without-paper/ewp-specs-api-iia-cnr/blob/stable-v3/manifest-entry.xsd"
    xmlns:ns12="https://github.com/erasmus-without-paper/ewp-specs-api-ounits/blob/stable-v2/manifest-entry.xsd"
    xmlns:ns13="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-none/tree/stable-v1"
    xmlns:ns14="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-httpsig/tree/stable-v1"
    xmlns:ns15="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-tlscert/tree/stable-v1"
    xmlns:ns16="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-httpsig/tree/stable-v1"
    xmlns:ns2="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1"
    xmlns:ns4="https://github.com/erasmus-without-paper/ewp-specs-sec-intro/tree/stable-v2"
    xmlns:ns5="https://github.com/erasmus-without-paper/ewp-specs-api-iias-approval/blob/stable-v2/manifest-entry.xsd"
    xmlns:ns6="https://github.com/erasmus-without-paper/ewp-specs-api-iia-approval-cnr/blob/stable-v2/manifest-entry.xsd"
    xmlns:ns7="https://github.com/erasmus-without-paper/ewp-specs-api-institutions/blob/stable-v2/manifest-entry.xsd"
    xmlns:ns8="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v2/manifest-entry.xsd"
    xmlns:ns9="https://github.com/erasmus-without-paper/ewp-specs-api-iias/blob/stable-v7/manifest-entry.xsd"
>
    <ns3:host>
        <admin-email>ewp-admin@aau.at</admin-email>
        <admin-provider>University of Klagenfurt</admin-provider>
        <admin-notes>University of Klagenfurt Staging System</admin-notes>
        <ns2:apis-implemented>
            <ns10:discovery version="6.0.0">
                <ns10:url>https://ewp-staging.aau.at/ewp/manifest</ns10:url>
            </ns10:discovery>
            <ns8:echo version="2.0.1">
                <ns8:http-security>
                    <ns4:client-auth-methods>
                        <ns14:httpsig/>
                    </ns4:client-auth-methods>
                    <ns4:server-auth-methods>
                        <ns15:tlscert/>
                        <ns16:httpsig/>
                    </ns4:server-auth-methods>
                </ns8:http-security>
                <ns8:url>https://ewp-staging.aau.at/ewp/echo</ns8:url>
            </ns8:echo>
            <ns7:institutions version="2.2.0">
                <ns7:http-security>
                    <ns4:client-auth-methods>
                        <ns14:httpsig/>
                    </ns4:client-auth-methods>
                    <ns4:server-auth-methods>
                        <ns15:tlscert/>
                        <ns16:httpsig/>
                    </ns4:server-auth-methods>
                </ns7:http-security>
                <ns7:url>https://ewp-staging.aau.at/ewp/institutions</ns7:url>
                <ns7:max-hei-ids>1</ns7:max-hei-ids>
            </ns7:institutions>
            <ns9:iias version="7.0.0">
                <ns9:http-security>
                    <ns4:client-auth-methods>
                        <ns14:httpsig/>
                    </ns4:client-auth-methods>
                    <ns4:server-auth-methods>
                        <ns16:httpsig/>
                    </ns4:server-auth-methods>
                </ns9:http-security>
                <ns9:get-url>https://ewp-staging.aau.at/ewp/iias/get</ns9:get-url>
                <ns9:max-iia-ids>10</ns9:max-iia-ids>
                <ns9:index-url>https://ewp-staging.aau.at/ewp/iias/index</ns9:index-url>
                <ns9:stats-url>https://ewp-staging.aau.at/ewp/iias/stats</ns9:stats-url>
            </ns9:iias>
            <ns5:iias-approval version="2.0.0">
                <ns5:http-security>
                    <ns4:client-auth-methods>
                        <ns14:httpsig/>
                    </ns4:client-auth-methods>
                    <ns4:server-auth-methods>
                        <ns16:httpsig/>
                    </ns4:server-auth-methods>
                </ns5:http-security>
                <ns5:url>https://ewp-staging.aau.at/ewp/iias/approval</ns5:url>
                <ns5:max-iia-ids>10</ns5:max-iia-ids>
            </ns5:iias-approval>
            <ns6:iia-approval-cnr version="2.0.0">
                <ns6:http-security>
                    <ns4:client-auth-methods>
                        <ns14:httpsig/>
                    </ns4:client-auth-methods>
                    <ns4:server-auth-methods>
                        <ns16:httpsig/>
                    </ns4:server-auth-methods>
                </ns6:http-security>
                <ns6:url>https://ewp-staging.aau.at/ewp/iias/approval/cnr</ns6:url>
            </ns6:iia-approval-cnr>
        </ns2:apis-implemented>
        <ns3:institutions-covered>
            <ns2:hei id="uni-klu.ac.at">
                <ns2:other-id type="pic">999836813</ns2:other-id>
                <ns2:other-id type="erasmus">A  KLAGENF01</ns2:other-id>
                <ns2:name xml:lang="de">Universit√§t Klagenfurt</ns2:name>
                <ns2:name xml:lang="en">University of Klagenfurt</ns2:name>
            </ns2:hei>
        </ns3:institutions-covered>
        <ns3:client-credentials-in-use>
            <ns3:rsa-public-key></ns3:rsa-public-key>
        </ns3:client-credentials-in-use>
        <ns3:server-credentials-in-use>
            <ns3:rsa-public-key></ns3:rsa-public-key>
        </ns3:server-credentials-in-use>
    </ns3:host>
</ns3:manifest>
