<dis6:manifest
    xmlns:dis6="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/tree/stable-v6"
    xmlns:cliauthsig1="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-httpsig/tree/stable-v1"
    xmlns:common1="https://github.com/erasmus-without-paper/ewp-specs-architecture/blob/stable-v1/common-types.xsd"
    xmlns:d6="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v6/manifest-entry.xsd"
    xmlns:e2="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v2/manifest-entry.xsd"
    xmlns:inst2="https://github.com/erasmus-without-paper/ewp-specs-api-institutions/blob/stable-v2/manifest-entry.xsd"
    xmlns:ounits2="https://github.com/erasmus-without-paper/ewp-specs-api-ounits/blob/stable-v2/manifest-entry.xsd"
    xmlns:reg1="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1"
    xmlns:sec1="https://github.com/erasmus-without-paper/ewp-specs-sec-intro/tree/stable-v2"
    xmlns:srvauthsig1="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-httpsig/tree/stable-v1"
    xmlns:srvauthtls1="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-tlscert/tree/stable-v1"
>
    <dis6:host>
        <common1:admin-email>oasis-ewp@ugent.be</common1:admin-email>
        <common1:admin-provider>Ghent University (OASIS)</common1:admin-provider>
        <reg1:apis-implemented>
            <e2:echo version="2.0.0">
                <e2:http-security>
                    <sec1:client-auth-methods>
                        <cliauthsig1:httpsig/>
                    </sec1:client-auth-methods>
                    <sec1:server-auth-methods>
                        <srvauthsig1:httpsig/>
                        <srvauthtls1:tlscert/>
                    </sec1:server-auth-methods>
                </e2:http-security>
                <e2:url>https://qas.ewp.ugent.be/secure-api/echo</e2:url>
            </e2:echo>
            <inst2:institutions version="2.0.0">
                <inst2:http-security>
                    <sec1:client-auth-methods>
                        <cliauthsig1:httpsig/>
                    </sec1:client-auth-methods>
                    <sec1:server-auth-methods>
                        <srvauthsig1:httpsig/>
                        <srvauthtls1:tlscert/>
                    </sec1:server-auth-methods>
                </inst2:http-security>
                <inst2:url>https://qas.ewp.ugent.be/secure-api/institutions</inst2:url>
                <inst2:max-hei-ids>1</inst2:max-hei-ids>
            </inst2:institutions>
            <ounits2:organizational-units version="2.0.0">
                <ounits2:http-security>
                    <sec1:client-auth-methods>
                        <cliauthsig1:httpsig/>
                    </sec1:client-auth-methods>
                    <sec1:server-auth-methods>
                        <srvauthsig1:httpsig/>
                        <srvauthtls1:tlscert/>
                    </sec1:server-auth-methods>
                </ounits2:http-security>
                <ounits2:url>https://qas.ewp.ugent.be/secure-api/ounits</ounits2:url>
                <ounits2:max-ounit-ids>1</ounits2:max-ounit-ids>
                <ounits2:max-ounit-codes>1</ounits2:max-ounit-codes>
            </ounits2:organizational-units>
            <d6:discovery version="6.0.0">
                <d6:url>https://qas.ewp.ugent.be/public-api/discovery/manifest.xml</d6:url>
            </d6:discovery>
        </reg1:apis-implemented>
        <dis6:institutions-covered>
            <reg1:hei id="ugent.be">
                <reg1:other-id type="erasmus">B  GENT01</reg1:other-id>
                <reg1:other-id type="pic">999986096</reg1:other-id>
                <reg1:name xml:lang="nl">Universiteit Gent</reg1:name>
                <reg1:name xml:lang="en">Ghent University</reg1:name>
            </reg1:hei>
        </dis6:institutions-covered>
        <dis6:client-credentials-in-use>
            <dis6:rsa-public-key></dis6:rsa-public-key>
        </dis6:client-credentials-in-use>
        <dis6:server-credentials-in-use>
            <dis6:rsa-public-key></dis6:rsa-public-key>
        </dis6:server-credentials-in-use>
    </dis6:host>
</dis6:manifest>
