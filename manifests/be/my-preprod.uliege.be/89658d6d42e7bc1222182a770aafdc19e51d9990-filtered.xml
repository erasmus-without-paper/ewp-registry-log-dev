<i:manifest
    xmlns:i="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/tree/stable-v5"
    xmlns:a="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v5/manifest-entry.xsd"
    xmlns:b="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-httpsig/tree/stable-v1"
    xmlns:c="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-tlscert/tree/stable-v1"
    xmlns:d="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v2/manifest-entry.xsd"
    xmlns:e="https://github.com/erasmus-without-paper/ewp-specs-api-iia-cnr/blob/stable-v2/manifest-entry.xsd"
    xmlns:f="https://github.com/erasmus-without-paper/ewp-specs-sec-intro/tree/stable-v2"
    xmlns:g="https://github.com/erasmus-without-paper/ewp-specs-api-institutions/blob/stable-v2/manifest-entry.xsd"
    xmlns:h="https://github.com/erasmus-without-paper/ewp-specs-api-iias/blob/stable-v4/manifest-entry.xsd"
    xmlns:j="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1"
    xmlns:k="https://github.com/erasmus-without-paper/ewp-specs-architecture/blob/stable-v1/common-types.xsd"
>
    <i:host>
        <k:admin-email>roland.francois@uliege.be</k:admin-email>
        <j:apis-implemented>
            <a:discovery version="5.0.0">
                <a:url>https://my-preprod.uliege.be/ws_penelope/EWP/manifest</a:url>
            </a:discovery>
            <d:echo version="2.0.1">
                <d:http-security>
                    <f:client-auth-methods>
                        <b:httpsig/>
                    </f:client-auth-methods>
                    <f:server-auth-methods>
                        <c:tlscert allows-self-signed="false"/>
                    </f:server-auth-methods>
                </d:http-security>
                <d:url>https://my-preprod.uliege.be/ws_penelope/EWP/echo</d:url>
            </d:echo>
            <g:institutions version="2.1.0">
                <g:http-security>
                    <f:client-auth-methods>
                        <b:httpsig/>
                    </f:client-auth-methods>
                    <f:server-auth-methods>
                        <c:tlscert allows-self-signed="false"/>
                    </f:server-auth-methods>
                </g:http-security>
                <g:url>https://my-preprod.uliege.be/ws_penelope/EWP/institutions</g:url>
                <g:max-hei-ids>10</g:max-hei-ids>
            </g:institutions>
            <h:iias version="4.0.0">
                <h:http-security>
                    <f:client-auth-methods>
                        <b:httpsig/>
                    </f:client-auth-methods>
                    <f:server-auth-methods>
                        <c:tlscert allows-self-signed="false"/>
                    </f:server-auth-methods>
                </h:http-security>
                <h:get-url>https://my-preprod.uliege.be/ws_penelope/EWP/iias/get</h:get-url>
                <h:max-iia-ids>10</h:max-iia-ids>
                <h:max-iia-codes>10</h:max-iia-codes>
                <h:index-url>https://my-preprod.uliege.be/ws_penelope/EWP/iias/index</h:index-url>
            </h:iias>
            <e:iia-cnr version="2.0.2">
                <e:http-security>
                    <f:client-auth-methods>
                        <b:httpsig/>
                    </f:client-auth-methods>
                    <f:server-auth-methods>
                        <c:tlscert allows-self-signed="false"/>
                    </f:server-auth-methods>
                </e:http-security>
                <e:url>https://my-preprod.uliege.be/ws_penelope/EWP/iias/cnr</e:url>
            </e:iia-cnr>
        </j:apis-implemented>
        <i:institutions-covered>
            <j:hei id="uliege.be">
                <j:other-id type="ERASMUS">B  LIEGE01</j:other-id>
                <j:name xml:lang="fr">Université de Liège</j:name>
                <j:name xml:lang="en">University of Liège</j:name>
            </j:hei>
        </i:institutions-covered>
        <i:client-credentials-in-use>
            <i:rsa-public-key></i:rsa-public-key>
        </i:client-credentials-in-use>
    </i:host>
</i:manifest>
