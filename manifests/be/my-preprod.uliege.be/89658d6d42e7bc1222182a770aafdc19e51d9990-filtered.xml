<k:manifest
    xmlns:k="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/tree/stable-v5"
    xmlns:a="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-tlscert/tree/stable-v1"
    xmlns:b="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-httpsig/tree/stable-v1"
    xmlns:c="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-httpsig/tree/stable-v1"
    xmlns:d="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-tlscert/tree/stable-v1"
    xmlns:e="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v5/manifest-entry.xsd"
    xmlns:f="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v2/manifest-entry.xsd"
    xmlns:g="https://github.com/erasmus-without-paper/ewp-specs-api-iia-cnr/blob/stable-v2/manifest-entry.xsd"
    xmlns:h="https://github.com/erasmus-without-paper/ewp-specs-sec-intro/tree/stable-v2"
    xmlns:i="https://github.com/erasmus-without-paper/ewp-specs-api-institutions/blob/stable-v2/manifest-entry.xsd"
    xmlns:j="https://github.com/erasmus-without-paper/ewp-specs-api-iias/blob/stable-v4/manifest-entry.xsd"
    xmlns:l="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1"
    xmlns:m="https://github.com/erasmus-without-paper/ewp-specs-architecture/blob/stable-v1/common-types.xsd"
>
    <k:host>
        <m:admin-email>roland.francois@uliege.be</m:admin-email>
        <l:apis-implemented>
            <e:discovery version="5.0.0">
                <e:url>https://my-preprod.uliege.be/ws_penelope/EWP/manifest</e:url>
            </e:discovery>
            <f:echo version="2.0.1">
                <f:http-security>
                    <h:client-auth-methods>
                        <c:httpsig/>
                    </h:client-auth-methods>
                    <h:server-auth-methods>
                        <d:tlscert/>
                    </h:server-auth-methods>
                </f:http-security>
                <f:url>https://my-preprod.uliege.be/ws_penelope/EWP/echo</f:url>
            </f:echo>
            <i:institutions version="2.1.0">
                <i:http-security>
                    <h:client-auth-methods>
                        <c:httpsig/>
                    </h:client-auth-methods>
                    <h:server-auth-methods>
                        <d:tlscert/>
                    </h:server-auth-methods>
                </i:http-security>
                <i:url>https://my-preprod.uliege.be/ws_penelope/EWP/institutions</i:url>
                <i:max-hei-ids>10</i:max-hei-ids>
            </i:institutions>
            <j:iias version="4.0.0">
                <j:http-security>
                    <h:client-auth-methods>
                        <c:httpsig/>
                    </h:client-auth-methods>
                    <h:server-auth-methods>
                        <d:tlscert/>
                    </h:server-auth-methods>
                </j:http-security>
                <j:get-url>https://my-preprod.uliege.be/ws_penelope/EWP/iias/get</j:get-url>
                <j:max-iia-ids>10</j:max-iia-ids>
                <j:max-iia-codes>10</j:max-iia-codes>
                <j:index-url>https://my-preprod.uliege.be/ws_penelope/EWP/iias/index</j:index-url>
            </j:iias>
            <g:iia-cnr version="2.0.2">
                <g:http-security>
                    <h:client-auth-methods>
                        <c:httpsig/>
                    </h:client-auth-methods>
                    <h:server-auth-methods>
                        <d:tlscert/>
                    </h:server-auth-methods>
                </g:http-security>
                <g:url>https://my-preprod.uliege.be/ws_penelope/EWP/iias/cnr</g:url>
            </g:iia-cnr>
        </l:apis-implemented>
        <k:institutions-covered>
            <l:hei id="uliege.be">
                <l:other-id type="ERASMUS">B  LIEGE01</l:other-id>
                <l:name xml:lang="fr">Université de Liège</l:name>
                <l:name xml:lang="en">University of Liège</l:name>
            </l:hei>
        </k:institutions-covered>
        <k:client-credentials-in-use>
            <k:rsa-public-key></k:rsa-public-key>
        </k:client-credentials-in-use>
    </k:host>
</k:manifest>
