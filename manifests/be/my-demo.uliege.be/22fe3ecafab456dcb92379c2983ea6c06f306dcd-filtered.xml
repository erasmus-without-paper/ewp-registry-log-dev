                <e:url>https://my-demo.uliege.be/ws_penelope/EWP/manifest</e:url>
            </e:discovery>
            <f:echo version="2.0.1">
                <f:http-security>
                    <i:client-auth-methods>
                        <b:httpsig/>
                    </i:client-auth-methods>
                    <i:server-auth-methods>
                        <d:tlscert/>
                    </i:server-auth-methods>
                </f:http-security>
                <f:url>https://my-demo.uliege.be/ws_penelope/EWP/echo</f:url>
