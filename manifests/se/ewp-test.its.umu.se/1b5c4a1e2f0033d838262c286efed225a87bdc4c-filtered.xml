<manifest xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/tree/stable-v5">
    <host>
        <admin-notes xmlns="https://github.com/erasmus-without-paper/ewp-specs-architecture/blob/stable-v1/common-types.xsd">This is a EWP reference connector instance.</admin-notes>
        <ns2:apis-implemented xmlns:ns2="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1">
            <ns10:omobility-cnr
                xmlns:ns10="https://github.com/erasmus-without-paper/ewp-specs-api-omobility-cnr/blob/stable-v1/manifest-entry.xsd"
                version="0.4.0"
            >
                <ns10:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/omobilities/cnr</ns10:url>
                <ns10:max-omobility-ids>1</ns10:max-omobility-ids>
            </ns10:omobility-cnr>
            <ns18:echo
                xmlns:ns18="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v2/manifest-entry.xsd"
                version="2.0.0"
            >
                <ns18:http-security>
                    <ns5:client-auth-methods xmlns:ns5="https://github.com/erasmus-without-paper/ewp-specs-sec-intro/tree/stable-v2">
                        <ns20:tlscert
                            xmlns:ns20="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-tlscert/tree/stable-v1"
                            allows-self-signed="true"
                        />
                        <ns22:httpsig xmlns:ns22="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-httpsig/tree/stable-v1"/>
                    </ns5:client-auth-methods>
                    <ns5:server-auth-methods xmlns:ns5="https://github.com/erasmus-without-paper/ewp-specs-sec-intro/tree/stable-v2">
                        <ns24:tlscert xmlns:ns24="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-tlscert/tree/stable-v1"/>
                        <ns23:httpsig xmlns:ns23="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-httpsig/tree/stable-v1"/>
                    </ns5:server-auth-methods>
                </ns18:http-security>
                <ns18:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/echo</ns18:url>
            </ns18:echo>
            <ns13:courses
                xmlns:ns13="https://github.com/erasmus-without-paper/ewp-specs-api-courses/blob/stable-v1/manifest-entry.xsd"
                version="0.7.0"
            >
                <ns13:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/courses</ns13:url>
                <ns13:max-los-ids>1</ns13:max-los-ids>
                <ns13:max-los-codes>1</ns13:max-los-codes>
            </ns13:courses>
            <ns19:discovery
                xmlns:ns19="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v4/manifest-entry.xsd"
                version="4.1.0"
            >
                <ns19:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/manifest</ns19:url>
            </ns19:discovery>
            <ns9:imobilities
                xmlns:ns9="https://github.com/erasmus-without-paper/ewp-specs-api-imobilities/blob/stable-v1/manifest-entry.xsd"
                version="0.1.0"
            >
                <ns9:get-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/imobilities/get</ns9:get-url>
                <ns9:max-omobility-ids>1</ns9:max-omobility-ids>
                <ns9:sends-notifications/>
            </ns9:imobilities>
            <ns17:institutions
                xmlns:ns17="https://github.com/erasmus-without-paper/ewp-specs-api-institutions/blob/stable-v2/manifest-entry.xsd"
                version="2.0.0"
            >
                <ns17:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/institutions</ns17:url>
                <ns17:max-hei-ids>1</ns17:max-hei-ids>
            </ns17:institutions>
            <ns15:iias
                xmlns:ns15="https://github.com/erasmus-without-paper/ewp-specs-api-iias/blob/stable-v2/manifest-entry.xsd"
                version="2.0.1"
            >
                <ns15:get-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/iias/get</ns15:get-url>
                <ns15:max-iia-ids>1</ns15:max-iia-ids>
                <ns15:max-iia-codes>1</ns15:max-iia-codes>
                <ns15:index-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/iias/index</ns15:index-url>
                <ns15:sends-notifications/>
            </ns15:iias>
            <ns14:iia-cnr
                xmlns:ns14="https://github.com/erasmus-without-paper/ewp-specs-api-iia-cnr/blob/stable-v2/manifest-entry.xsd"
                version="2.0.1"
            >
                <ns14:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/iias/cnr</ns14:url>
            </ns14:iia-cnr>
            <ns6:imobility-tor-cnr
                xmlns:ns6="https://github.com/erasmus-without-paper/ewp-specs-api-imobility-tor-cnr/blob/stable-v1/manifest-entry.xsd"
                version="0.1.0"
            >
                <ns6:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/imobilities/tors/cnr</ns6:url>
                <ns6:max-omobility-ids>1</ns6:max-omobility-ids>
            </ns6:imobility-tor-cnr>
            <ns7:imobility-tors
                xmlns:ns7="https://github.com/erasmus-without-paper/ewp-specs-api-imobility-tors/blob/stable-v1/manifest-entry.xsd"
                version="0.7.0"
            >
                <ns7:get-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/imobilities/tors/get</ns7:get-url>
                <ns7:index-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/imobilities/tors/index</ns7:index-url>
                <ns7:max-omobility-ids>1</ns7:max-omobility-ids>
                <ns7:sends-notifications/>
            </ns7:imobility-tors>
            <ns12:simple-course-replication
                xmlns:ns12="https://github.com/erasmus-without-paper/ewp-specs-api-course-replication/blob/stable-v1/manifest-entry.xsd"
                version="1.0.0"
            >
                <ns12:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/courses/replication</ns12:url>
                <ns12:allows-anonymous-access>true</ns12:allows-anonymous-access>
                <ns12:supports-modified-since>false</ns12:supports-modified-since>
            </ns12:simple-course-replication>
            <ns16:organizational-units
                xmlns:ns16="https://github.com/erasmus-without-paper/ewp-specs-api-ounits/blob/stable-v2/manifest-entry.xsd"
                version="2.0.0"
            >
                <ns16:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/ounits</ns16:url>
                <ns16:max-ounit-ids>1</ns16:max-ounit-ids>
                <ns16:max-ounit-codes>1</ns16:max-ounit-codes>
            </ns16:organizational-units>
            <ns8:imobility-cnr
                xmlns:ns8="https://github.com/erasmus-without-paper/ewp-specs-api-imobility-cnr/blob/stable-v1/manifest-entry.xsd"
                version="0.1.0"
            >
                <ns8:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/imobilities/cnr</ns8:url>
                <ns8:max-omobility-ids>1</ns8:max-omobility-ids>
            </ns8:imobility-cnr>
        </ns2:apis-implemented>
        <institutions-covered>
            <ns2:hei
                xmlns:ns2="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1"
                id="ikea.university.se"
            >
                <ns2:other-id type="erasmus">S Ikea01</ns2:other-id>
                <ns2:other-id type="local">IK1234</ns2:other-id>
                <ns2:name xml:lang="sv">IKEA universitet</ns2:name>
                <ns2:name xml:lang="en">IKEA university</ns2:name>
            </ns2:hei>
        </institutions-covered>
    </host>
</manifest>
