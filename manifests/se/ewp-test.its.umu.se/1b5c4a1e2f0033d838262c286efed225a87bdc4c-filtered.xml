                    <ns18:tlscert/>
                    <ns17:httpsig/>
                </ns5:server-auth-methods>
            </ns12:http-security>
            <ns12:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/echo</ns12:url>
        </ns12:echo>
        <ns6:omobilities version="0.11.0">
            <ns6:get-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/mobilities/get</ns6:get-url>
            <ns6:index-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/mobilities/index</ns6:index-url>
            <ns6:update-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/mobilities/update</ns6:update-url>
            <ns6:max-omobility-ids>1</ns6:max-omobility-ids>
            <ns6:sends-notifications/>
            <ns6:supported-update-types/>
        </ns6:omobilities>
        <ns11:institutions version="2.0.0">
            <ns11:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/institutions</ns11:url>
            <ns11:max-hei-ids>1</ns11:max-hei-ids>
        </ns11:institutions>
        <ns8:courses version="0.7.0">
            <ns8:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/courses</ns8:url>
            <ns8:max-los-ids>1</ns8:max-los-ids>
            <ns8:max-los-codes>1</ns8:max-los-codes>
        </ns8:courses>
