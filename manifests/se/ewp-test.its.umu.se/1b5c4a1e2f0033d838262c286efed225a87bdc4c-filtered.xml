        <ns9:imobilities version="0.1.0">
            <ns9:get-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/imobilities/get</ns9:get-url>
            <ns9:max-omobility-ids>1</ns9:max-omobility-ids>
            <ns9:sends-notifications/>
        </ns9:imobilities>
        <ns15:iias version="2.0.1">
            <ns15:get-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/iias/get</ns15:get-url>
            <ns15:max-iia-ids>1</ns15:max-iia-ids>
            <ns15:max-iia-codes>1</ns15:max-iia-codes>
            <ns15:index-url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/iias/index</ns15:index-url>
            <ns15:sends-notifications/>
        </ns15:iias>
        <ns6:imobility-tor-cnr version="0.1.0">
            <ns6:url>https://ewp-test.its.umu.se/ewp-reference-connector/rest/imobilities/tors/cnr</ns6:url>
            <ns6:max-omobility-ids>1</ns6:max-omobility-ids>
