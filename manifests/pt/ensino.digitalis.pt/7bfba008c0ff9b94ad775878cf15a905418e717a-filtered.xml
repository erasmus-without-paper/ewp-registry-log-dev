<ns3:manifest
    xmlns:ns3="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/tree/stable-v5"
    xmlns="https://github.com/erasmus-without-paper/ewp-specs-architecture/blob/stable-v1/common-types.xsd"
    xmlns:ns10="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v2/manifest-entry.xsd"
    xmlns:ns11="https://github.com/erasmus-without-paper/ewp-specs-api-institutions/blob/stable-v2/manifest-entry.xsd"
    xmlns:ns12="https://github.com/erasmus-without-paper/ewp-specs-api-institutions/tree/stable-v2"
    xmlns:ns13="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-tlscert/tree/stable-v1"
    xmlns:ns14="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-httpsig/tree/stable-v1"
    xmlns:ns15="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-httpsig/tree/stable-v1"
    xmlns:ns16="https://github.com/erasmus-without-paper/ewp-specs-sec-reqencr-tls/tree/stable-v1"
    xmlns:ns17="https://github.com/erasmus-without-paper/ewp-specs-sec-resencr-tls/tree/stable-v1"
    xmlns:ns18="https://github.com/erasmus-without-paper/ewp-specs-sec-cliauth-tlscert/tree/stable-v1"
    xmlns:ns19="https://github.com/erasmus-without-paper/ewp-specs-types-academic-term/tree/stable-v1"
    xmlns:ns2="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1"
    xmlns:ns4="https://github.com/erasmus-without-paper/ewp-specs-api-echo/tree/stable-v2"
    xmlns:ns5="https://github.com/erasmus-without-paper/ewp-specs-types-contact/tree/stable-v1"
    xmlns:ns6="https://github.com/erasmus-without-paper/ewp-specs-types-phonenumber/tree/stable-v1"
    xmlns:ns7="https://github.com/erasmus-without-paper/ewp-specs-types-address/tree/stable-v1"
    xmlns:ns8="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v5/manifest-entry.xsd"
    xmlns:ns9="https://github.com/erasmus-without-paper/ewp-specs-sec-intro/tree/stable-v2"
>
    <ns3:host>
        <admin-email>lpinto@digitalis.pt</admin-email>
        <admin-notes>MOBILIDADEnet EWP Manifest</admin-notes>
        <ns2:apis-implemented>
            <ns8:discovery version="5.0.0">
                <ns8:url>https://ensino.digitalis.pt/mobilidadenet/doc/ewpstage/manifest</ns8:url>
            </ns8:discovery>
            <ns10:echo version="2.0.1">
                <admin-email>lpinto@digitalis.pt</admin-email>
                <ns10:http-security>
                    <ns9:client-auth-methods>
                        <ns14:httpsig/>
                    </ns9:client-auth-methods>
                    <ns9:server-auth-methods>
                        <ns15:httpsig/>
                        <ns13:tlscert/>
                    </ns9:server-auth-methods>
                    <ns9:request-encryption-methods>
                        <ns16:tls/>
                    </ns9:request-encryption-methods>
                    <ns9:response-encryption-methods>
                        <ns17:tls/>
                    </ns9:response-encryption-methods>
                </ns10:http-security>
                <ns10:url>https://ensino.digitalis.pt/mobilidadenet/doc/ewpstage/echo</ns10:url>
            </ns10:echo>
            <ns11:institutions version="2.1.0">
                <admin-email>lpinto@digitalis.pt</admin-email>
                <ns11:http-security>
                    <ns9:client-auth-methods>
                        <ns14:httpsig/>
                    </ns9:client-auth-methods>
                    <ns9:server-auth-methods>
                        <ns15:httpsig/>
                        <ns13:tlscert/>
                    </ns9:server-auth-methods>
                    <ns9:request-encryption-methods>
                        <ns16:tls/>
                    </ns9:request-encryption-methods>
                    <ns9:response-encryption-methods>
                        <ns17:tls/>
                    </ns9:response-encryption-methods>
                </ns11:http-security>
                <ns11:url>https://ensino.digitalis.pt/mobilidadenet/doc/ewpstage/institutions</ns11:url>
                <ns11:max-hei-ids>1</ns11:max-hei-ids>
            </ns11:institutions>
        </ns2:apis-implemented>
        <ns3:institutions-covered>
            <ns2:hei id="digitalis.pt">
                <ns2:other-id type="SCHAC">digitalis.pt</ns2:other-id>
                <ns2:name xml:lang="en">Escola Superior de Qualidade Digitalis</ns2:name>
            </ns2:hei>
        </ns3:institutions-covered>
        <ns3:client-credentials-in-use>
            <ns3:rsa-public-key></ns3:rsa-public-key>
        </ns3:client-credentials-in-use>
        <ns3:server-credentials-in-use>
            <ns3:rsa-public-key></ns3:rsa-public-key>
        </ns3:server-credentials-in-use>
    </ns3:host>
</ns3:manifest>
