    <ewp:admin-email>sistemas@sigmaaie.org</ewp:admin-email>
    <ewp:admin-email>arquitectura@sigmaaie.org</ewp:admin-email>
    <ewp:admin-notes>SIGMA SaaS EWP Dev Manifest</ewp:admin-notes>

    <r:apis-implemented>
        <discovery
            xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v4/manifest-entry.xsd"
            version="4.0.0"
        >
            <url>https://erasmuswithoutpaper.sigmaaie.org:1453/UPF_HOST.xml</url>
        </discovery>

        <!-- Some other API. -->
        <echo
            xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v1/manifest-entry.xsd"
            version="2.0.0"
        >
            <!-- Note, that ewp:admin-email and ewp:admin-notes can be defined both on the
            global level, and on the API level. -->
            <ewp:admin-email>arquitectura@sigmaaie.org</ewp:admin-email>
            <ewp:admin-email>sistemas@sigmaaie.org</ewp:admin-email>
            <ewp:admin-notes>SIGMA SaaS Echo API Dev.</ewp:admin-notes>

            <!-- This element is defined by the Echo API (in its manifest-entry.xsd file).
            Most APIs will require you to provide the `version` attribute and a single `url`
            element only, but it's NOT a rule. Some APIs may require you to provide completely
            other properties (which should be described in their manifest-entry.xsd file). -->
            <url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/echo</url>
        </echo>
        <ns4:institutions version="2.0.0">
            <ns4:url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/institutions/get</ns4:url>
            <ns4:max-hei-ids>1</ns4:max-hei-ids>
        </ns4:institutions>
        <ns2:organizational-units version="2.0.0">
            <ns2:url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/departments/get</ns2:url>
            <ns2:max-ounit-ids>100</ns2:max-ounit-ids>
            <ns2:max-ounit-codes>100</ns2:max-ounit-codes>
        </ns2:organizational-units>
        <ns3:omobilities version="0.12.1">
            <ns3:get-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/omobility/get</ns3:get-url>
            <ns3:index-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/omobility/index</ns3:index-url>
            <ns3:update-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/omobility/update</ns3:update-url>            
            <ns3:max-omobility-ids>1</ns3:max-omobility-ids>
            <ns3:supported-update-types/>
        </ns3:omobilities>
        <ns5:iias version="2.0.0">
          <ns5:get-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/iias/get</ns5:get-url>
          <ns5:max-iia-ids>1</ns5:max-iia-ids>
          <ns5:max-iia-codes>1</ns5:max-iia-codes>
          <ns5:index-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/iias/index</ns5:index-url>
        </ns5:iias>
         <ns6:courses version="0.7.0">
            <ns6:url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/courses</ns6:url>
            <ns6:max-los-ids>1</ns6:max-los-ids>
            <ns6:max-los-codes>1</ns6:max-los-codes>
        </ns6:courses>
        <ns7:imobility-tors version="0.7.0">
          <ns7:get-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/tor/get</ns7:get-url>
          <ns7:index-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/tor/index</ns7:index-url>
          <ns7:max-omobility-ids>1</ns7:max-omobility-ids>
        </ns7:imobility-tors>
        <ns8:simple-course-replication version="1.0.0">
          <ns8:url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/course-replication</ns8:url>
          <ns8:allows-anonymous-access>false</ns8:allows-anonymous-access>
          <ns8:supports-modified-since>false</ns8:supports-modified-since>
        </ns8:simple-course-replication>
    </r:apis-implemented>

    <mf:institutions-covered xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1">   
        <hei id="upf.edu">
            <other-id type="erasmus">E_BARCELO15</other-id>                    
            <name xml:lang="es">UNIVERSITAT POMPEU FABRA</name>
            <name xml:lang="en">POMPEU FABRA UNIVERSITY</name>
        </hei>          
    </mf:institutions-covered>
   

