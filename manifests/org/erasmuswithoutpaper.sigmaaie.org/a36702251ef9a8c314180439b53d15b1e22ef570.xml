    <ewp:admin-email>arquitectura@sigmaaie.org</ewp:admin-email>
    <ewp:admin-notes>SIGMA UVa EWP Dev Manifest</ewp:admin-notes>

    <r:apis-implemented>
        <discovery
            xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/blob/stable-v4/manifest-entry.xsd"
            version="4.0.0"
        >
            <url>https://erasmuswithoutpaper.sigmaaie.org:1453/UVA_HOST.xml</url>
        </discovery>

        <!-- Some other API. -->
        <echo
            xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-echo/blob/stable-v1/manifest-entry.xsd"
            version="1.0.0"
        >
            <!-- Note, that ewp:admin-email and ewp:admin-notes can be defined both on the
            global level, and on the API level. -->
            <ewp:admin-email>arquitectura@sigmaaie.org</ewp:admin-email>
            <ewp:admin-email>sistemas@sigmaaie.org</ewp:admin-email>
            <ewp:admin-notes>SIGMA UVa Echo API Dev.</ewp:admin-notes>

            <!-- This element is defined by the Echo API (in its manifest-entry.xsd file).
            Most APIs will require you to provide the `version` attribute and a single `url`
            element only, but it's NOT a rule. Some APIs may require you to provide completely
            other properties (which should be described in their manifest-entry.xsd file). -->
            <url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/echo</url>
        </echo>
        <ns4:institutions version="1.0.0">
            <ns4:url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/institutions/get</ns4:url>
            <ns4:max-hei-ids>1</ns4:max-hei-ids>
        </ns4:institutions>
        <ns2:organizational-units version="1.0.0">
            <ns2:url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/departments/get</ns2:url>
            <ns2:max-ounit-ids>n</ns2:max-ounit-ids>
            <ns2:max-ounit-codes>n</ns2:max-ounit-codes>
        </ns2:organizational-units>
        <ns3:mobilities version="1.0.0">
            <ns3:get-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/mobility/get</ns3:get-url>
            <ns3:index-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/mobilityIndex/get</ns3:index-url>            
            <ns3:max-mobility-ids>1</ns3:max-mobility-ids>
            <ns3:supported-update-types/>
        </ns3:mobilities>
         <ns3:courses version="1.0.0">
            <ns3:get-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/courses/get</ns3:get-url>
            <ns3:index-url>https://erasmuswithoutpaper.sigmaaie.org:1453/ewp/rest/allCourses/get</ns3:index-url>            
            <ns3:max-mobility-ids>1</ns3:max-mobility-ids>
            <ns3:supported-update-types/>
        </ns3:courses>
    </r:apis-implemented>

    <mf:institutions-covered xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1">   
